{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@data/*": ["data/*"],
      "@types/*": ["types/*"],
      "@constants/*": ["constants/*"],
      "@components/*": ["components/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}